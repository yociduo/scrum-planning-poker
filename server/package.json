{
  "name": "scrum-planning-poker",
  "version": "3.3.2",
  "description": "scrum-planning-poker server",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "nodemon --watch /src/**/* -e ts,tsx --exec ts-node src/app.ts",
    "build-ts": "tsc -p tsconfig.build.json",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve": "cross-env NODE_ENV=production node dist/app.js",
    "build": "npm run tslint && npm run build-ts",
    "watch": "npm run dev",
    "test": "jest --env=node --colors --coverage=false --detectOpenHandles tests",
    "test:coverage": "jest --env=node --colors --coverage tests"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yociduo/scrum-planning-poker.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yociduo/scrum-planning-poker/issues"
  },
  "homepage": "https://www.scrumplanning.cn",
  "author": "",
  "dependencies": {
    "axios": "^0.21.1",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.20",
    "log4js": "^6.1.0",
    "multer": "^1.4.0",
    "mysql": "^2.16.0",
    "node-forge": "^0.8.2",
    "pg": "^7.11.0",
    "reflect-metadata": "^0.1.12",
    "routing-controllers": "^0.7.7",
    "socket-controllers": "^0.0.5",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.26",
    "typeorm-routing-controllers-extensions": "^0.2.0",
    "typeorm-typedi-extensions": "^0.2.3"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^24.0.24",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/koa": "^2.0.48",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.32",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.11.3",
    "@types/node-forge": "^0.8.2",
    "@types/shelljs": "^0.8.0",
    "@types/socket.io": "^1.4.38",
    "cross-env": "^5.2.0",
    "jest": "^25.5.3",
    "nodemon": "^2.0.3",
    "ts-jest": "^25.4.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.4.5"
  }
}
